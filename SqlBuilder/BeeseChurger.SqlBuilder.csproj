    <PackageId>BeeseChurger.SqlBuilder</PackageId>
    <Version>1.1.2</Version>
    <Authors>SMistry7</Authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <RepositoryUrl>https://github.com/smistry7/SqlBuilder</RepositoryUrl>
